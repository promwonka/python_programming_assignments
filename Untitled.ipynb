    "def word_sort() :\n",
    "    my_str = (input(\"Enter the number\"))\n",
    "    words = [word.lower() for word in my_str.split()]\n",
    "    words.sort()\n",
    "    return words\n",
    "\n",
    "def output_w():\n",
    "    words = word_sort()\n",
    "    print(\"The sorted words are:\")\n",
    "    for word in words:\n",
    "       print(word)\n",
